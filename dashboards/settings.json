t\tvar devtype = document.getElementById(\"devicetype\").value\r\n\r\n\t\tif (devid !== \"Device ID\" \u0026\u0026 desc !== \"Bezeichnung\" \u0026\u0026 devtype !== \"\" ) {\r\n\t\t\tdocument.getElementById(\"savedev\").disabled = false;\r\n\t\t}\r\n\r\n\t};\r\n\r\n\tvar showdev = function () {\r\n\t\tvar showdev_str = \"devmgmt show\";\r\n\t\tpublish_settings(showdev_str);\r\n\t};\r\n\r\n\tvar adddev = function () {\r\n\t\tvar input_payload = \"\u003cfont size='5'\u003eGer√§t wird hinzugef√ºgt\u003c/font\u003e\u003cbr\u003e\u003cbr\u003e\u003cfont size='4'\u003eBitte warten...\u003c/font\u003e\";\r\n\t\t$(\"#settings_output\").html(input_payload);\r\n\r\n\t\tvar devid = document.getElementById(\"devid\").value;\r\n\t\tvar desc = document.getElementById(\"desc\").value;\r\n\t\tvar devicetype = document.getElementById(\"devicetype\").value;\r\n\r\n\t\tvar adddev_str = \"devmgmt add '\" + devid + \"' '\" + desc + \"' '\" + devicetype + \"'\";\r\n\r\n\t\tpublish_settings(adddev_str);\r\n\t};\r\n\r\n\tvar deldev = function () {\r\n\t\tvar input_payload = \"\u003cfont size='5'\u003eGer√§t wird gel√∂scht\u003c/font\u003e\u003cbr\u003e\u003cbr\u003e\u003cfont size='4'\u003eBitte warten...\u003c/font\u003e\";\r\n\t\t$(\"#settings_output\").html(input_payload);\r\n\r\n\t\tvar devid = document.getElementById(\"devlist\").value;\r\n\t\tvar text = document.getElementById(\"devlist\");\r\n\t\tvar desc = text.options[text.selectedIndex].text;\r\n\t\tvar remdev_str = \"devmgmt remove '\" + devid + \"' '\" + desc + \"'\";\r\n\r\n\t\tpublish_settings(remdev_str);\r\n\t};\r\n\u003c/script\u003e\r\n","mode":"html"},"pluginVersion":"10.1.2","targets":[{"datasource":{"type":"influxdb","uid":"8iwjhe17z"},"refId":"A"}],"type":"text"},{"datasource":{"type":"influxdb","uid":"8iwjhe17z"},"gridPos":{"h":30,"w":8,"x":5,"y":0},"id":26,"links":[],"options":{"code":{"language":"plaintext","showLineNumbers":false,"showMiniMap":false},"content":"\u003ciframe id=\"show_dev\" overflow=\"hidden\" scro